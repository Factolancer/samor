@import "../../public/styles/partials/web-partials";

.main-header {
  color: $white;
  background-color: $brand-secondary;
  text-align: center;
  font-size: 18px;
  font-weight: 700;
  line-height: 85px;
  margin: 0 !important;
}

.other-header {
  line-height: 64px;
  background-color: $dark-grey;
  color: $white;
  margin: 0 !important;
  text-align: center;
}

.main-content {
  font-size: 16px;
  font-weight: 700;
  //text-align: center;
  color: $black;
  border-bottom: solid 1px $dark-grey;
  background-color: $light-grey;
  margin: 0 !important;
  padding: 20px 0;
}

.other-content {
  font-size: 16px;
  text-align: center;
  color: $dark-grey;
  background-color: $light-grey;
  padding: 26px 0;
  border-bottom: solid 1px $dark-grey;
  margin: 0 !important;
}

.background-secondary {
  background-color: $brand-secondary;
  color: $white;
}

.white-circle {
  @include circle(24px, $white);
}

.lightgray-circle {
  @include circle(42px, $light-grey);
}

.saving-plus-icon {
  color: $brand-secondary;
}

.heading-32-cash {
  @include heading(32px, 300);

  .imp {
    color: $brand-primary;
    font-weight: 400;
  }
}

.heading-16-cash {
  @include heading(16px, 300);
  .imp {
    color: $brand-primary;
    font-weight: 400;
  }
}

.cash-fund-card {
    background-color: $light-grey;
    color: $black;
}

.cash-fund-name {
    @include heading(16px, 400);
    color: $brand-secondary;
}

.dark-grey {
  color: $dark-grey;
}

.mar-tb-5 {
  margin-top: 5px;
  margin-bottom: 5px;
}

.grey-border {
  border: 1px solid $dark-grey;
}

md-card{
  border: 1px $dark-grey;
}

