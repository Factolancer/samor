<div class="row wrapper">
    <div class="col-sm-10 col-sm-offset-1">
        <app-user-journey></app-user-journey>
        <div class="row">
            <div class="col-sm-12 center-align heading-32-blue pad-tb-15">Invest in SmartSIP equity schemes</div>
        </div>
        <div class="row grey-back pad-20">
            <app-smart-sip-card class="col-sm-12" #smartSipFunds [funds]="quickSipFunds"></app-smart-sip-card>
        </div>
        <div class="row">
            <div class="col-sm-2 col-sm-offset-5">
                <div class="product-cta">
                    <button class="cta-50" color="primary" md-raised-button=""
                            (click)="proceed(smartSipFunds.selectedFunds)">Proceed
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>