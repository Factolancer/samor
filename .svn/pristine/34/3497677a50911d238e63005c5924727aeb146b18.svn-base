    <!--<text-overlay fxFlex="100" fxAlign="center" [overlayText]="balanceSheetMessage"></text-overlay>-->

    <br/>
    <div fxFlex="40">
        <div fxLayout="column" fxLayoutAlign="center center">
            <md-card fxFlex class="no-margin">
                <md-card-content fxLayout="row" fxLayoutAlign="center center">
                    <div class="heading-24 pad-20 background-secondary" fxAlign="center">
                        Lets Have a look at what the Earning and Expenses of an indiviuals is like
                    </div>
                </md-card-content>
            </md-card>
            <md-card fxFlex class="no-margin">
                <md-card-content class="grey-back main-content" fxLayout="row" fxLayoutAlign="center center">
                    <div fxFlex class="main-content pad-20">Date</div>
                    <div fxFlex class="main-content">Cash Flow</div>
                    <div fxFlex class="main-content">Amount</div>
                    <div fxFlex class="main-content">Account Balance</div>
                </md-card-content>
            </md-card>
            <md-card class="no-margin grey-border">
                <md-card-content fxLayout="column">
                    <div fxLayout="row" fxLayoutAlign="center center">
                        <div fxFlex class="pad-10">{{'1 Jan' | date:'d MMM'}}</div>
                        <div fxFlex class="pad-10">Salary</div>
                        <div fxFlex class="pad-10">&#8377; 70,000</div>
                        <div fxFlex class="pad-10">&#8377; 70,000</div>
                    </div>
                    <div fxLayout="row">
                        <div fxFlex class="pad-10">{{'9 Jan' | date:'d MMM'}}</div>
                        <div fxFlex class="pad-10">Expense 1</div>
                        <div fxFlex class="pad-10">&#8377; 20,000</div>
                        <div fxFlex class="pad-10">&#8377; 50,000</div>
                    </div>
                    <div fxLayout="row">
                        <div fxFlex class="pad-10">{{'15 Jan' | date:'d MMM'}}</div>
                        <div fxFlex class="pad-10">Expense 2</div>
                        <div fxFlex class="pad-10">&#8377; 10,000</div>
                        <div fxFlex class="pad-10">&#8377; 40,000</div>
                    </div>
                </md-card-content>
            </md-card>
            <md-card class="no-margin grey-border">
                <md-card-content fxLayout="column">
                    <div fxLayout="row" fxLayoutAlign="center center">
                        <div fxFlex class="pad-10">{{'1 Jan' | date:'d MMM'}}</div>
                        <div fxFlex class="pad-10">Salary</div>
                        <div fxFlex class="pad-10">&#8377; 70,000</div>
                        <div fxFlex class="pad-10">&#8377; 70,000</div>
                    </div>
                    <div fxLayout="row">
                        <div fxFlex class="pad-10">{{'9 Jan' | date:'d MMM'}}</div>
                        <div fxFlex class="pad-10">Expense 1</div>
                        <div fxFlex class="pad-10">&#8377; 20,000</div>
                        <div fxFlex class="pad-10">&#8377; 50,000</div>
                    </div>
                    <div fxLayout="row">
                        <div fxFlex class="pad-10">{{'15 Jan' | date:'d MMM'}}</div>
                        <div fxFlex class="pad-10">Expense 2</div>
                        <div fxFlex class="pad-10">&#8377; 10,000</div>
                        <div fxFlex class="pad-10">&#8377; 40,000</div>
                    </div>
                </md-card-content>
            </md-card>
            <md-card class="no-margin grey-border">
                <md-card-content fxLayout="column">
                    <div fxLayout="row" fxLayoutAlign="center center">
                        <div fxFlex class="pad-10">{{'1 Jan' | date:'d MMM'}}</div>
                        <div fxFlex class="pad-10">Salary</div>
                        <div fxFlex class="pad-10">&#8377; 70,000</div>
                        <div fxFlex class="pad-10">&#8377; 70,000</div>
                    </div>
                    <div fxLayout="row">
                        <div fxFlex class="pad-10">{{'9 Jan' | date:'d MMM'}}</div>
                        <div fxFlex class="pad-10">Expense 1</div>
                        <div fxFlex class="pad-10">&#8377; 20,000</div>
                        <div fxFlex class="pad-10">&#8377; 50,000</div>
                    </div>
                    <div fxLayout="row">
                        <div fxFlex class="pad-10">{{'15 Jan' | date:'d MMM'}}</div>
                        <div fxFlex class="pad-10">Expense 2</div>
                        <div fxFlex class="pad-10">&#8377; 10,000</div>
                        <div fxFlex class="pad-10">&#8377; 40,000</div>
                    </div>
                </md-card-content>
            </md-card>
        </div>
    </div>

</div>


