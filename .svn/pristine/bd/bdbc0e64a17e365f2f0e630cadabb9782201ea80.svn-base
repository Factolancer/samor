<md-card _ngcontent-pxi-6 style="width: 80%; align-self: center;">
    <md-toolbar color="primary">Basic Details</md-toolbar>
    <br/>
    <md-card-content>
        <form #basicForm="ngForm" (ngSubmit)="onBasicSubmit()">
            <md-grid-list cols="2" [style.background]="'lightblue'" rowHeight="100px">
                <md-grid-tile>
                    <md-input id="pan" type="text" placeholder="PAN" required minlength="10" maxlength="10"
                              name="pan" ngControl="pan" [(ngModel)]="user.Basic.pan" #pan="ngModel" [ngModelOptions]="{standalone: true}">
                        <!--<md-hint *ngIf="formErrors.pan" align="end" class="alert alert-danger">{{formErrors.pan}}</md-hint>-->
                    </md-input>
                </md-grid-tile>
                <md-grid-tile>
                    <md-input id="mob" type="number" placeholder="Mobile No." required minlength="10" maxlength="10"
                              name="mob" ngControl="mob" [(ngModel)]="user.Basic.mob" #mob="ngModel" [ngModelOptions]="{standalone: true}"></md-input>
                </md-grid-tile>
            </md-grid-list>
            <md-grid-list cols="2" [style.background]="'lightblue'" rowHeight="100px">
                <md-grid-tile>
                    <md-input id="name" type="text" placeholder="Name" required
                              name="name" ngControl="name" [(ngModel)]="user.Basic.name" #name="ngModel" [ngModelOptions]="{standalone: true}"></md-input>
                </md-grid-tile>
                <md-grid-tile>
                    <md-input id="email" type="email" placeholder="Email Id" required
                              name="email" ngControl="email" [(ngModel)]="user.Basic.email" #email="ngModel" [ngModelOptions]="{standalone: true}"></md-input>
                </md-grid-tile>
            </md-grid-list>
            <md-grid-list cols="2" [style.background]="'lightblue'" rowHeight="100px">
                <md-grid-tile>
                    <md-input id="dob" type="date" required ngControl="dob"
                              name="dob" [(ngModel)]="user.Basic.dob" #dob="ngModel" [ngModelOptions]="{standalone: true}"></md-input>
                    <label _ngcontent-lwp-8 class="md-input-placeholder md-float" required>Date of Birth</label>
                </md-grid-tile>
                <md-grid-tile>
                    <md-radio-group id="gender" required ngControl="gender" [(ngModel)]="user.Basic.gender" name="gender" #gender="ngModel" [ngModelOptions]="{standalone: true}">
                        <md-radio-button value="Male">Male</md-radio-button>
                        <md-radio-button value="Female">Female</md-radio-button>
                    </md-radio-group>
                </md-grid-tile>
            </md-grid-list>
            <md-grid-list cols="2" [style.background]="'lightblue'" rowHeight="100px">
                <md-grid-tile>
                    <md-radio-group id="marital_status" ngControl="marital_status" [(ngModel)]="user.Basic.marital_status" name="marital_status" #marital_status="ngModel" [ngModelOptions]="{standalone: true}">
                        <md-radio-button value="Single">Single</md-radio-button>
                        <md-radio-button value="Married">Married</md-radio-button>
                    </md-radio-group>
                </md-grid-tile>
                <md-grid-tile>
                    <button md-raised-button type="submit" color="primary" [disabled]="!basicForm.form.valid">{{!basicForm.form.valid}}</button>
                </md-grid-tile>
            </md-grid-list>
        </form>
    </md-card-content>
</md-card>

<md-card _ngcontent-pxi-6 style="width: 80%; align-self: center;">
    <md-toolbar color="primary">Know Your Customer Details</md-toolbar>
    <br/>
    <md-card-content>
        <form #kycForm="ngForm" (ngSubmit)="onKYCSubmit()" novalidate>
            <md-grid-list cols="2" [style.background]="'lightblue'" rowHeight="100px">
                <md-grid-tile>
                    <select class="form-control" name="Occupation" >
                        <option *ngFor="let item of occupations" [value]="item">{{item.name}}</option>
                    </select>
                </md-grid-tile>
                <md-grid-tile>
                    <md-input type="number" placeholder="Annual Income(in lakhs)"></md-input>
                </md-grid-tile>
            </md-grid-list>
            <md-grid-list cols="2" [style.background]="'lightblue'" rowHeight="100px">
                <md-grid-tile>
                    <md-input type="text" placeholder="Father/Spouse Name"></md-input>
                </md-grid-tile>
                <md-grid-tile>
                    <select class="form-control" name="Address Proof">
                        <option *ngFor="let item of addressprooofs" [value]="item">{{item.name}}</option>
                    </select>
                </md-grid-tile>
            </md-grid-list>
            <md-grid-list cols="2" [style.background]="'lightblue'" rowHeight="100px">
                <md-grid-tile>
                    <md-input type="text" placeholder="House Number, Street Name"></md-input>
                </md-grid-tile>
                <md-grid-tile>
                    <md-input type="text" placeholder="Town/District"></md-input>
                </md-grid-tile>
            </md-grid-list>
            <md-grid-list cols="2" [style.background]="'lightblue'" rowHeight="100px">
                <md-grid-tile>
                    <md-input type="text" placeholder="State"></md-input>
                </md-grid-tile>
                <md-grid-tile>
                    <md-input type="number" placeholder="Pin Code" minlength="6" maxlength="6"></md-input>
                </md-grid-tile>
            </md-grid-list>
            <md-grid-list cols="2" [style.background]="'lightblue'" rowHeight="100px">
                <md-grid-tile>

                </md-grid-tile>
                <md-grid-tile>
                    <button md-raised-button color="primary">Save</button>
                </md-grid-tile>
            </md-grid-list>
        </form>
    </md-card-content>
</md-card>

<md-card _ngcontent-pxi-6 style="width: 80%; align-self: center;">
    <md-toolbar color="primary">Know Your Customer Details</md-toolbar>
    <br/>
    <md-card-content>
        <form>
            <md-grid-list cols="2" [style.background]="'lightblue'" rowHeight="100px">
                <md-grid-tile>
                    <md-input type="text" placeholder="Name as on Cheque Book"></md-input>
                </md-grid-tile>
                <md-grid-tile>
                    <md-input type="text" placeholder="IFSC Code"></md-input>
                </md-grid-tile>
            </md-grid-list>
            <md-grid-list cols="2" [style.background]="'lightblue'" rowHeight="100px">
                <md-grid-tile>
                    <select class="form-control" name="Bank Account Type">
                        <option>Savings Account</option>
                        <option>Current Account</option>
                    </select>
                </md-grid-tile>
                <md-grid-tile>
                    <md-input type="number" placeholder="Bank Account Number"></md-input>
                </md-grid-tile>
            </md-grid-list>
            <md-grid-list cols="2" [style.background]="'lightblue'" rowHeight="100px">
                <md-grid-tile>

                </md-grid-tile>
                <md-grid-tile>
                    <button md-raised-button color="primary">Save</button>
                </md-grid-tile>
            </md-grid-list>
        </form>
    </md-card-content>
</md-card>

