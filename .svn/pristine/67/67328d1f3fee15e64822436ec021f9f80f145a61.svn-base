<app-user-journey [position]=1></app-user-journey>
<div fxLayout="row" fxLayoutAlign="center center" class="pad-tb-25 wrapper">
    <div fxFlex="30" class="center-align">
        <div class="heading-32-dark-grey border-down">Increase your earnings</div>
        <hr/>
        <div class="sub-heading-16-medium-grey">by Investing in SavingsPlus</div>
    </div>
</div>
<div fxLayout="row" fxLayoutAlign="center center" class="wrapper">
    <div fxFlex="80" class="grey-back pad-30">
        <app-savings-plus-card [funds]="savingsPlusFunds"></app-savings-plus-card>
        <md-card class="fund-card">
            <md-card-content>
                <div fxLayout="row" fxLayoutAlign="start center">
                    <div fxFlex="5" class="lightgray-circle">  <!--[@ShowHide]="iconOne"-->
                        <md-icon class="md-24 md-brand-secondary">trending_up</md-icon>
                    </div>
                    <div fxFlexOffset="2" fxAlign="center">
                        Higher returns as compared to Savings and Deposits
                    </div>
                    <div fxFlex="5" class="lightgray-circle">
                        <md-icon class="md-24 md-brand-secondary">update</md-icon>
                    </div>
                    <div fxFlexOffset="2" fxAlign="center">
                        Same day redemption
                    </div>
                    <div fxFlex="5" class="lightgray-circle">
                        <md-icon class="md-24 md-brand-secondary">lock_open</md-icon>
                    </div>
                    <div fxFlexOffset="2" fxAlign="center">
                        No lock-in or exit load
                    </div>
                    <div fxFlex="5" class="lightgray-circle">
                        <md-icon class="md-24 md-brand-secondary">&#xE876;</md-icon>
                    </div>
                    <div fxFlexOffset="2" fxAlign="center">
                        Invest and redeem on a click!
                    </div>
