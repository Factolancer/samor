@import "../../public/styles/partials/web-partials";

.section-header {
  background-color: $brand-secondary;
  color: $white;
  @include heading(20px, 300);
  width: 100%;
  line-height: 50px;
}

md-icon{
  cursor: pointer;
}

.section-content {
  background-color: $white;
  width: 100%;
}

md-input-container{
  width: 90%;
}

.errorMessage {
  float: right;
  margin-top: 1px;
  font-size: 0.7em;
  color: red;

}
.errorMessage span {
  position: absolute;
  color: red;
  top: -10px;
  font-size:0.63em;
}
