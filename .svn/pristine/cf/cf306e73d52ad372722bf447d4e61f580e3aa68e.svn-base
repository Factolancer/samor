@import "../../styles/partials/web-partials";


.static{

  padding-top: 30px;
  padding-bottom: 20px;

  .title{
    font-size: 36px;
    font-weight: bold;
    letter-spacing: 1.9px;
    color: #5f5f5f;
    text-align: center;
    text-transform: uppercase;margin-bottom: 20px;
  }
  .content{
    background-color: #fafafa;
    padding: 30px;

    .heading{
      text-transform: uppercase;
      font-size: 14px;
      letter-spacing: 0.7px;
      color:#49c2bb;
    }
    .text{
      text-align: justify;
      font-size: 12px;
      line-height: 20px;
      letter-spacing: 0.8px;
      color:#5f5f5f;
      margin-bottom: 20px;
    }
    .bold{font-weight: 600;}

  }
}