<div class="wrapper" fxLayout="row" fxLayoutAlign="center center">
    <div fxFlex="55">
        <div fxLayout="row" fxLayoutAlign="center center" class="pad-tb-25">
            <div fxFlex="80" class="center-align">
                <div class="heading-32-dark-grey">Registration & KYC
                </div>
                <div class="sub-heading-16-medium-grey" style="padding-bottom: 15px;">
                    <hr/>
                    Registration Complete
                </div>
            </div>
        </div>

        <div class="grey-border pad-lr-30">
            <div>
                <div fxFlex="100" fxFlexOffset="5" class="heading-24-blue pad-tb-15">
                    Thank you!
                </div>
                <br/>
                <div fxFlex="100" fxFlexOffset="5" class="heading-24-bold">
                    Your Fincash registration is complete
                </div>
                <br/>
                <br/>
                <br/>
                <div fxFlex="100" fxFlexOffset="5" class="heading-16-grey pad-tb-10">
                    What Next ?
                </div>
            </div>
        </div>
        <div class="bottom-section">
            <div>
                <div fxFlexOffset="5" class="pad-lr-30 sub-heading-16-medium-grey">
                    Our team will get in touch with you to activate your Fincash Account for investing
                </div>
            </div>

        </div>
        <br/>

        <div fxLayout="row" fxLayoutAlign="start center" class="mar-tb-5">
            <button fxFlex="30" class="cta-50" md-raised-button color="primary" (click)="completeRegistration()">Explore out Products</button>
            <button fxFlex="30" fxFlexOffset="40" class="cta-50" md-raised-button color="primary" (click)="toDashboard()">Go to Dashboard</button>
        </div>
    </div>
</div>
