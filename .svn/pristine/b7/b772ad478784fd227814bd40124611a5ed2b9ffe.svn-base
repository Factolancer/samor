<div class="row col-sm-10 col-sm-offset-1">
    <app-savings-plus-card class="col-sm-12" [funds]="savingsPlusFunds"></app-savings-plus-card>
    <div class="col-sm-12">
        <md-card class="cash-fund-card">
            <md-card-content class="center-align">
                <div class="col-sm-3 no-padding">
                    <div class="col-sm-2">
                        <div class="white-circle">
                            <i class="material-icons md-brand-secondary">trending_up</i>
                        </div>
                    </div>
                    <div class="col-sm-10">
                        Higher returns as compared to Savings and Deposits
                    </div>
                </div>

                <div class="col-sm-3 no-padding">
                    <div class="col-sm-2">
                        <div class="white-circle">
                            <i class="material-icons md-brand-secondary">update</i>
                        </div>
                    </div>
                    <div class="col-sm-10">
                        Same day redemption
                    </div>
                </div>

                <div class="col-sm-3 no-padding">
                    <div class="col-sm-2">
                        <div class="white-circle">
                            <i class="material-icons md-brand-secondary">lock_open</i>
                        </div>
                    </div>
                    <div class="col-sm-10">
                        No lock-in or exit load
                    </div>
                </div>

                <div class="col-sm-3 no-padding">
                    <div class="col-sm-2">
                        <div class="white-circle">
                            <i class="material-icons md-brand-secondary">check_box</i>
                        </div>
                    </div>
                    <div class="col-sm-10">
                        Invest and redeem on a click!
                    </div>
                </div>
            </md-card-content>
        </md-card>
    </div>
</div>
<br/>
<div class="row col-sm-10 col-sm-offset-1">

</div>


