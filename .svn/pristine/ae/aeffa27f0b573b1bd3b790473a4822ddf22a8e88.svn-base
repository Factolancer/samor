<div class="data-body">
    <div class="col-md-8 col-md-offset-2">
        <table class="table">
            <thead class="table-header">
                <th class="table-element">Folio</th>
                <th class="table-element">Fund</th>
                <th class="table-element">IRR</th>
            </thead>
            <tbody>
            <tr *ngFor="let data of irrData">
                <td class="table-element">{{data.folioNo}}</td>
                <td class="table-element">{{data.fundName}}</td>
                <td class="table-element">{{data.xirr | percent}}</td>
            </tr>
            </tbody>
        </table>
    </div>
</div>