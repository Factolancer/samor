md-card{
    width: 100%;
    margin: 10px;
    display: inline;
    padding: 0;
}
md-card-content{
    height: 100%;
}

table{
    border-collapse: collapse;
    height: 100%;
    width: 100%;
}
thead{
    background-color: #0081c9 ;
    color: white;
    height: 10%;
    text-align: center;
}
tbody{
    border: 2px solid white;
    background-color: #f7f7f7;
    padding: 4px;
    height: 30%;
}
tr td, th{
    padding: 5px;
    text-align: center;
    border: 1px solid white;
}

.green{
    color: green;
}
.red{
    color: red;
    text-align: right;
}
.darkblue{
    color: darkblue;
    text-align: right;
}

.screenheading{
    color: #0081c9;
    font-size: 200%;
}