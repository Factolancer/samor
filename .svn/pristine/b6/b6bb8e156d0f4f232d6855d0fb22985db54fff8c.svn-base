md-card{
    width: 100%;
    padding: 0;
    display: inline;
}
md-card-content{
    height: 100%;
}

section{
    margin-top: 10px;
    margin-bottom: 10px;
}

table{
    border-collapse: collapse;
    height: 100%;
    width: 100%;
}
thead{
    background-color: #0081c9 ;
    color: white;
    height: 10%;
    text-align: center;
}
tbody{
    border: 2px solid white;
    background-color: #f9f9f9;
    padding: 4px;
    height: 30%;
}
tr td, th{
    padding: 5px;
}

.screenheading{
    color: #0081c9;
    font-size: 200%;
}
